{
  "name": "pomodoro-app",
  "version": "1.0.0",
  "description": "A simple pomodoro timer app to focus on stuff that actually matters.",
  "repository": "git@github.com:once-upon-a-tam/pomodoro-app.git",
  "author": "Tam <tam.pro@outlook.fr>",
  "homepage": "once-upon-a-tam.github.io/pomodoro-app/",
  "default": "./index.html",
  "scripts": {
    "start": "parcel ./index.html",
    "build": "parcel build ./index.html --public-url ./",
    "predeploy": "rm -rf dist && parcel build ./index.html --no-source-maps --public-url ./",
    "deploy": "gh-pages -d dist"
  },
  "assetsPath": "assets",
  "dependencies": {
    "gh-pages": "^4.0.0",
    "sass": "^1.54.3"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.7.0",
    "parcel": "^2.7.0"
  }
}
